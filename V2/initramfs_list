#links
slink /bin/sh /bin/busybox 777 0 0
slink /bin/cd /bin/busybox 777 0 0
slink /bin/ln /bin/busybox 777 0 0 

#busybox

file /bin/busybox ../systemsoftware/V2/systemarm/bin/busybox 755 0 0

